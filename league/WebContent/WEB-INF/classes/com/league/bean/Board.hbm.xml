<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.league.bean.Board" table="board">
			<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>

		<property name="author" type="java.lang.String" column="author"
			length="30" />
		<property name="daytime" type="java.lang.String" column="daytime"
			length="30" />

		<property name="context" type="java.lang.String" column="context"
			length="10000" />

	</class>
</hibernate-mapping>	