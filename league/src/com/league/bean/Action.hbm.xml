<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.league.bean.Action" table="action">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>

		<property name="actionname" type="java.lang.String" column="actionname"
			length="50" />
		<property name="league" type="java.lang.String" column="league"
			length="30" />
		<property name="actioncontext" type="java.lang.String"
			column="actioncontext" length="5000" />
		<property name="sponsor" type="java.lang.String" column="sponsor"
			length="20" />
		<property name="time" type="java.lang.String" column="time"
			length="100" />
		<property name="btext" type="java.lang.String" column="btext"
			length="1000" />
		<property name="checked" type="java.lang.String" column="checked"
			length="2" />
	</class>
</hibernate-mapping>	